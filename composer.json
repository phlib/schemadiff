{
    "name": "phlib/schemadiff",
    "description": "Schema Diff: Show difference between MySQL databases",
    "type": "library",
    "license": "LGPL-3.0",
    "homepage": "https://github.com/phlib/schemadiff",
    "keywords": [
        "mysql",
        "schema",
        "diff"
    ],
    "authors": [
        {
            "name": "James Dempster"
        },
        {
            "name": "Phlib Team & Contributors",
            "homepage": "https://github.com/phlib/schemadiff/contributors"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "symfony/console": "^6 || ^7"
    },
    "autoload": {
        "psr-4": {
            "Phlib\\SchemaDiff\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "symplify/easy-coding-standard": "^12"
    },
    "autoload-dev": {
        "psr-4": {
            "Phlib\\SchemaDiff\\Test\\": "tests"
        }
    },
    "bin": [
        "bin/schemadiff"
    ],
    "scripts": {
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": "vendor/bin/ecs check --fix --ansi"
    }
}
