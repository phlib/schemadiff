{
    "name": "phlib/schemadiff",
    "description": "Schema Diff: Show difference between MySQL databases",
    "type": "library",
    "license": "LGPL-3.0",
    "homepage": "https://github.com/phlib/schemadiff",
    "keywords": [
        "mysql",
        "schema",
        "diff"
    ],
    "authors": [
        {
            "name": "James Dempster"
        },
        {
            "name": "Phlib Team & Contributors",
            "homepage": "https://github.com/phlib/schemadiff/contributors"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "symfony/console": "^3.2|^4|^5"
    },
    "autoload": {
        "psr-4": {
            "Phlib\\SchemaDiff\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "symplify/easy-coding-standard": "^11"
    },
    "autoload-dev": {
        "psr-4": {
            "Phlib\\SchemaDiff\\Test\\": "tests"
        }
    },
    "bin": ["bin/schemadiff"]
}
